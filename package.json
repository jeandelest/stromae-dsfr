{
  "name": "stromae-dsfr",
  "private": true,
  "version": "0.0.36",
  "type": "module",
  "scripts": {
    "prepare": "vite-envs update-types",
    "dev": "vite",
    "check": "tsc --noEmit",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "_format": "prettier '**/*.{ts,tsx,json,md}'",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --list-different",
    "postinstall": "copy-dsfr-to-public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons",
    "openapi": "orval --config ./orval.config.ts",
    "resolve-schema": "node ./script/resolveSchema.js"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.9.11",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@inseefr/lunatic": "^3.0.9",
    "@inseefr/lunatic-dsfr": "^2.0.10",
    "@mui/material": "^5.15.18",
    "@tanstack/react-query": "^5.37.1",
    "@tanstack/react-router": "^1.33.7",
    "axios": "^1.7.2",
    "oidc-spa": "^4.6.1",
    "powerhooks": "^1.0.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-toastify": "^10.0.5",
    "tsafe": "^1.6.6",
    "tss-react": "^4.9.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.6.2",
    "@hookform/devtools": "^4.3.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "orval": "^6.29.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-envs": "^4.3.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
