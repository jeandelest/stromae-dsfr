{
  "name": "stromae-dsfr",
  "private": true,
  "version": "0.0.12",
  "type": "module",
  "scripts": {
    "prepare": "vite-envs update-types",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "_format": "prettier '**/*.{ts,tsx,json,md}'",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --list-different",
    "postinstall": "copy-dsfr-to-public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons && npx @inseefr/lunatic"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.9.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@inseefr/lunatic": "3.0.0-rc.13",
    "@inseefr/lunatic-dsfr": "2.0.0-rc.6",
    "@mui/material": "^5.15.12",
    "@tanstack/react-query": "^5.24.6",
    "@tanstack/react-router": "^1.18.3",
    "axios": "^1.6.7",
    "oidc-spa": "^4.2.1",
    "powerhooks": "^1.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "tsafe": "^1.6.6",
    "tss-react": "^4.9.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-envs": "^4.0.10",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
